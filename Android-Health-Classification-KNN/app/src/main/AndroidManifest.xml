        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".mvp.motherpregnanttraining.MotherPregnantTrainingActivity"></activity>
        <activity android:name=".mvp.village.VillageActivity" />
        <activity android:name=".mvp.toddlertraining.ToddlerTrainingActivity" />
        <activity android:name=".mvp.motherpregnantpercentage.MotherPregnantPercentageActivity" />
        <activity android:name=".mvp.motherpregnantresultclassification.MotherPregnantResultClassificationActivity" />
        <activity android:name=".mvp.motherpregnantclassification.MotherPregnantClassificationActivity" />
        <activity android:name=".mvp.mainpregnant.MainPregnantActivity" />
        <activity android:name=".mvp.toddlerrpercentage.PercentageToddlerActivity" />
